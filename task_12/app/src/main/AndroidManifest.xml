<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.task_12">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Task_12">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MasterActivity">
            <intent-filter>
                <action android:name="android.intent.action.MASTER" />
            </intent-filter>
        </activity>
        <activity android:name=".CreativeActivity">
            <intent-filter>
                <action android:name="android.intent.action.CREATIVE" />
            </intent-filter>
        </activity>
    </application>

</manifest>